#basics 
	42;
	true;false;
#structs
	(64,true);
#vars
	my::test::var;
#if
	if 0 then 1 else 2;
#while
	while 0 then 1;
#new
	new 1;
#new[]
	new[0] 2;
#scope
	{
		1;
		auto b=2;
	};
#math
	1+2*3;
#funclit
	${int_t.0} arg 0;
#return
	return 0;
#slice
	0[0..0];
#char literal
	'b';
#string literal
	"hello world";
#array literal
	[1,2,3];
