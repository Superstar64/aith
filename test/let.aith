template inc = x => x + 1;


~ world -> (& (&&), world &) ~
symbol main = do {
	nat = 1 : natural32;
	int = 2 : integer32;
	run assert(nat.(inc`) == 2);
	run assert(int.(inc`) == 3);
	()
};
