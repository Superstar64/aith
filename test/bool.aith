inline yes = true;

branch(b :: boolean) {
    if b {
        1
    } else {
        2
    }
}

complex(b) {
    if {if (b) { true } else {false} } {
        1
    } else {
        if yes {
            2
        } else {
            4
        }
    }
}

not(b) {
    !b
}

inBounds(x1, x2, x3) {
    x1 <= x2 & x2 < x3
}