alias void = tuple();

alias copy = > {
	assert($@_0.length == $@_1.length);
	let index = 0;
	while(index < $@_0.length) then {
		$@_1[index] = $@_0[index];
		index = index + 1;
	};
};

alias main :: void -> void = > {
	let base = [(true,true),(true,false),(false,false)];
	let target = new[3]((false,false));
	copy(base,target);
	assert(target == [(true,true),(true,false),(false,false)]);
};

alias assert :: boolean -> void = extern "assert";
