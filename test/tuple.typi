~ world -> world ~
symbol main = os {
	triplet = (true,false,1 : integer32);
	|os| = assert(triplet_0 == true, os);
	|os| = assert(triplet_1 == false, os);
	|os| = assert(triplet_2 == 1, os);
	os
};

~ (& boolean, world &) -> world ~
assert = extern "assert";
